{
  "name": "react-server-side-webpack2",
  "version": "1.0.0",
  "description": "Sample react with koa repo",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build-server && npm run build-client",
    "build-server": "cross-env BABEL_ENV=server webpack --config ./webpack.server.prod.config.js",
    "build-client": "cross-env BABEL_ENV=browser-prod webpack --config ./webpack.prod.config.js",
    "dev": "npm-run-all --parallel dev-server-build dev-server-start dev-client",
    "dev-server-build": "cross-env BABEL_ENV=server webpack --watch --config ./webpack.server.config.js",
    "dev-server-start": "cd build && nodemon ./server.js --watch ./",
    "dev-client": "cross-env BABEL_ENV=browser webpack-dev-server"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.10",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.11.2",
    "image-webpack-loader": "^3.6.0",
    "nodemon": "^1.17.1",
    "npm-run-all": "^4.1.2",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^2.7.0",
    "webpack-dev-server": "^2.11.2"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "bootstrap": "^4.0.0",
    "jquery": "^3.3.1",
    "koa": "^2.5.0",
    "koa-bodyparser": "^4.2.0",
    "koa-mount": "^3.0.0",
    "koa-router": "^7.4.0",
    "koa-static": "^3.0.0",
    "react": "^16.2.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.2.0",
    "react-easy-swipe": "0.0.13",
    "react-grid-system": "^3.1.2",
    "react-hot-loader": "^3.0.0",
    "react-hover": "^1.3.2",
    "react-image": "^1.3.1",
    "react-images": "^0.5.17",
    "react-measure": "^2.0.2",
    "react-photo-gallery": "^6.0.22",
    "react-redux": "^5.0.7",
    "react-responsive": "^4.0.5",
    "react-responsive-carousel": "^3.1.33",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "react-scroll": "^1.7.7",
    "react-slick": "^0.16.0",
    "react-transition-group": "^2.2.1",
    "reactstrap": "^5.0.0-beta.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "video-react": "^0.9.4"
  }
}
